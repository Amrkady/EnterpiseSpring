<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
	version="2.3">

	<factory>
		<exception-handler-factory>customeExceptions.CustomExceptionHandlerFactory</exception-handler-factory>
	</factory>
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>

		<locale-config>
			<default-locale>ar</default-locale>
		</locale-config>
		<resource-bundle>
			<base-name>common.util.I18nRessourceBundle</base-name>
			<var>msg</var>
		</resource-bundle>
	</application>
	<managed-bean>
		<managed-bean-name>employeesBean</managed-bean-name>
		<managed-bean-class>com.beans.EmployeesBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
			<property-name>departmentServiceImpl</property-name>
			<property-class>com.services.DepartmentServiceImpl</property-class>
			<value>#{departmentServiceImpl}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>revenueBean</managed-bean-name>
		<managed-bean-class>com.beans.RevenueBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
			<property-name>departmentServiceImpl</property-name>
			<property-class>com.services.DepartmentServiceImpl</property-class>
			<value>#{departmentServiceImpl}</value>
		</managed-property>
	</managed-bean>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/pages/dashboard.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/pages/dashboard.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>users</from-outcome>
			<to-view-id>/pages/usersScreen.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>financialYear</from-outcome>
			<to-view-id>/pages/financialYear.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>expensisType</from-outcome>
			<to-view-id>/pages/expensisTypeScreen.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>addEditEmp</from-outcome>
			<to-view-id>/pages/addEditEmp.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>employees</from-outcome>
			<to-view-id>/pages/employees.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>company</from-outcome>
			<to-view-id>/pages/company.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>contracts</from-outcome>
			<to-view-id>/pages/contracts.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>vacations</from-outcome>
			<to-view-id>/pages/vacations.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>expenses</from-outcome>
			<to-view-id>/pages/expenses.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>revenues</from-outcome>
			<to-view-id>/pages/revenues.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>employeesSalary</from-outcome>
			<to-view-id>/pages/employeesSalary.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>taxs</from-outcome>
			<to-view-id>/pages/taxs.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
</faces-config>

